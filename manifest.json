{
  "name": "tweet tab",
  "version": "0.1",
  "description": "tab complete for twitter",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "popup":"popup.html"
  },
  "permissions": [
    "http://*.twitter.com/*",
    "tabs"
  ],
  "content_scripts": [
    {
        "matches":["http://twitter.com/","http://twitter.com/home"],
        "js": [
            "jquery.js",
            "script.js",
            "jquery.autocomplete.js",
            "oauth.js",
            "sha1.js"
        ],
        "css": [
            "jquery.autocomplete.css"
        ]    
    }    
  ]    
}
