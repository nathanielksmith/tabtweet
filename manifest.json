{
  "name": "tab tweet",
  "version": "1.0",
  "description": "tab complete for twitter",
  "background_page" : "background.html",
  "options_page": "options.html",
  "icons": {
    "32" : "icon32.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "permissions": [
    "http://*.twitter.com/*",
    "tabs"
  ],
  "content_scripts": [
    {
        "matches":["http://twitter.com/","http://twitter.com/home"],
        "js": [
            "jquery.js",
            "script.js",
            "jquery.autocomplete.js",
            "oauth.js",
            "sha1.js"
        ],
        "css": [
            "jquery.autocomplete.css"
        ]    
    }    
  ]    
}
